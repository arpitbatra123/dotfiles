# aliases
alias ll='ls -l'
# edit the zsh shell config
alias ez='code ~/Documents/dotfiles'
# load the shell config
alias sz='source ~/.zshrc'
alias vi='nvim'
alias startdb='brew services start mysql@5.7'
alias stopdb='brew services stop mysql@5.7'
alias startredis='brew services start redis;'
alias ytd='youtube-dl'
alias python='python3'
alias pip='pip3'
# dump installed packages to brew file
alias brewdump='brew bundle dump --force'
# open VSCode even faster
alias c.='code .'

# git aliases
alias gco='git checkout'
alias gp='git pull'
alias gs='git status -sb'
alias gst='git stash'
alias gsa='git stash apply'

# node aliases
alias nr='npm run'
alias ncu='npx npm-check-updates -u'

# extra
alias xbrew="arch -x86_64 /usr/local/bin/brew"